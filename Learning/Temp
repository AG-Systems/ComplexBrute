 int gen()
{
	for (int i = 0; i < 500; i++)
	{
		// put an alg that goes a-z as the loop goes thou. ex: if i = 1 then you will get a
		char z = temp[1];
		z++;

		std::string str(1, z);

		temp.replace(1, 2, str);
		if (z >= 97 & z <= 122)

		std::cout << temp << std::endl;

		
		
	}
	return 0;
 }


int alg()
{
	// replaces the string with gen function
	
	for(int j = 0; j < 500; j++)
		
		 pass.replace(4, 1, temp);
		 

		
	
	return 0;


}

